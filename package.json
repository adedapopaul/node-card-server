{
  "name": "airtel_server",
  "version": "0.0.1",
  "description": "Airtel NG Card Generator server",
  "main": "dist",
  "scripts": {
    "start": "babel-node  src/index.js",
    "dev": " nodemon -w src --exec \"babel-node src \"",
    "build": "babel src -s -D -d dist,stage-0",
    "prestart": "npm run -s build",
    "lint": "eslint src",
    "test": "cross-env NODE_ENV=development mocha -r esm \"./{,!(node_modules)/**/}*.test.js\" --exit",
    "test2": "mocha \"./tests/**/*.js\" --recursive",
    "coverage": "cross-env NODE_ENV=development nyc --reporter=text npm test"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "husky": {
      "hooks": {
        "pre-commit": "pretty-quick --staged"
      }
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Dapo Aderemi <adedapopaul@yahoo.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.2.0",
    "eslint": "^6.0.1",
    "husky": "^2.4.1",
    "istanbul": "^0.4.5",
    "mocha": "^6.1.4",
    "nodemon": "^1.11.0",
    "nyc": "^14.1.1",
    "pretty-quick": "^1.11.1",
    "rewire": "^4.0.1",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "body-parser": "^1.15.2",
    "eslint": "^6.0.1",
    "esm": "^3.2.25",
    "express": "^4.14.0",
    "express-jwt": "^5.1.0",
    "jsonwebtoken": "^7.1.9",
    "mongoose": "^4.6.6",
    "nodemailer": "^4.6.8",
    "nodemon": "^1.11.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.0.0",
    "path": "^0.12.7",
    "randomatic": "^3.1.0"
  },
  "keywords": [
    "express",
    "es6",
    "rest",
    "api"
  ]
}
